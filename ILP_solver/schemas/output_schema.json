{
    "$schema": "https://json-schema.org/draft-07/schema#",
    "title": "schema for the output to be recieved from the web request",
    "type": "object",
    "properties": {
        "pairings" : { 
                "type" : "array",
                "items": {
                        "type" : "object",
                        "properties": {
                            "table_id" : {
                                        "type" : "integer"
                            },
                            "groups" : {
                                        "type" : "array",
                                        "items" : {
                                                "description": "the names of the groups associated with the table",
                                                "type" :"string"
                                        }
                            }
                        }

                }

        },
        "used_tables": {
            "type" : "integer",
            "minimum": 1
        }
    }

}